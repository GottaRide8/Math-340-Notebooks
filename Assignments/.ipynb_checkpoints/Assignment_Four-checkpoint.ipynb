    "def cos_x2(x,tol):\n",
    "    tot = 1.\n",
    "    aj = tot\n",
    "    jj = 1.\n",
    "    x4 = x**4.\n",
    "    while np.abs(aj) >= tol:\n",
    "        aj *= -x4/(2.*jj*(2.*jj-1.))\n",
    "        tot += aj\n",
    "        jj += 1.\n",
    "    return tot"
